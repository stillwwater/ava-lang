abs :: (a: int) -> int do
    if a < 0 then
        return -a
    end
    return a
end

fib :: (n: int) -> int do
    z: int
    a := 0
    b := 1
    i := 0

    while i < n do
        z = a + b
        a = b
        b = z
        i = i + 1
    end

    return a
end

factorial :: (n: int) -> int do
    if n == 0 then
        return 1
    else
        return factorial(n - 1)
    end
end


main :: (args: [] str) do
    abs(-12)
    fib(20)
    factorial(6)
end
